{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for react redux applications",
  "scripts": {
    "build": "rimraf build && webpack -p --config webpack.production.config.js --progress --colors",
    "clean": "rimraf build",
    "lint": "eslint ./src",
    "start": "webpack-dev-server --content-base public/ --config webpack.config.js --inline --hot",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --require babel-polyfill --require @babel/register src/**/*.spec.js -r mochaSetup.js",
    "watch": "webpack --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^6.4.0",
    "history": "^4.7.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.1.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.0",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^16.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-md5-hash": "0.0.6"
  }
}
